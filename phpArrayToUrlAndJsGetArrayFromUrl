<script type="text/javascript" src="http://code.jquery.com/jquery-2.1.3.min.js"></script>
<script type="text/javascript">
    $(document).ready(function() {
        var selectedRules = getUrlParams('params');
        var selectedRules = decodeURIComponent(selectedRules);
        var selectedRules = atob(selectedRules);
        var selectedRules = JSON.parse(selectedRules);

        console.log(selectedRules);// Object { param1: Array[2], param2: Array[2] }

        function getUrlParams(paramName) {
            var url = window.location.search.substring(1);
            var params = url.split('&');
            for (var i = 0; i < params.length; i++) {
                var param = params[i].split('=');
                if (param[0] == paramName) {
                    return param[1];
                }
            }
        }
    });
</script>
